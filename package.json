{
  "name": "@toolhouseai/n8n-nodes-toolhouse",
  "version": "1.0.7",
  "main": "index.js",
  "scripts": {
    "build": "npx rimraf dist && tsc && npm run copy:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix",
    "copy:icons": "copyfiles -u 1 'nodes/**/*.svg' dist/nodes && copyfiles -u 1 'credentials/**/*.svg' dist/credentials",
    "prepublishOnly": "npm run build && npm run lint -c .eslintrc.prepublish.js nodes credentials package.json"
  },
  "keywords": [
    "agents",
    "n8n",
    "n8n-community-node-package",
    "n8n-node",
    "toolhouse",
    "toolhouse-ai"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/toolhouseai/n8n-nodes-toolhouse.git"
  },
  "author": {
    "name": "Toolhouse AI",
    "email": "hello@toolhouse.ai"
  },
  "license": "MIT",
  "description": "Toolhouse AI nodes for n8n",
  "engines": {
    "node": ">=20.15"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/ToolhouseApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Toolhouse/Toolhouse.node.js",
      "dist/nodes/ToolhouseWebhook/ToolhouseWebhook.node.js"
    ],
    "icon": "dist/nodes/toolhouse.svg"
  },
  "devDependencies": {
    "@types/node": "^14.14.37",
    "@typescript-eslint/parser": "^8.37.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.3",
    "typescript": "^4.2.4",
    "copyfiles": "^2.4.1"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}
