{
  "name": "@toolhouseai/n8n-nodes-toolhouse",
  "version": "1.0.1",
  "main": "index.js",
  "scripts": {
    "build": "npx rimraf dist && tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix",
    "prepublishOnly": "npm run build && npm run lint -c .eslintrc.prepublish.js nodes credentials package.json"
  },
  "keywords": [
    "agents",
    "n8n",
    "n8n-community-node-package",
    "n8n-node",
    "toolhouse",
    "toolhouse-ai"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/toolhouseai/toolhouse-n8n.git"
  },
  "author": {
    "name": "Toolhouse AI",
    "email": "hello@toolhouse.ai"
  },
  "license": "MIT",
  "description": "Toolhouse AI nodes for n8n",
  "dependencies": {
    "axios": "^1.10.0"
  },
  "engines": {
    "node": ">=20.15"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/ToolhouseApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Toolhouse.node.js",
      "dist/nodes/ToolhouseWebhook.node.js"
    ],
    "icon": "dist/nodes/toolhouse.svg"
  },
  "devDependencies": {
    "@typescript-eslint/parser": "^8.37.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.3",
    "gulp": "^5.0.1",
    "n8n-core": "^1.14.1",
    "n8n-workflow": "^1.82.0",
    "typescript": "^5.8.3"
  }
}
